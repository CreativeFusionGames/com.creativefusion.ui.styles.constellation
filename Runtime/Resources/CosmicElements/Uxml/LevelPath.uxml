<ui:ListView name="path-nodes" class="path-nodes" selection-type="Single" virtualization-method="DynamicHeight"/>
</ui:VisualElement>


<ui:VisualElement class="layout-column centered card gap-8" name="path-detail" style="flex-grow:1;">
<ui:Label class="ce-kicker" text="Dimension 5"/>
<ui:Label class="ce-title-lg" text="Crystal Caverns"/>
<ui:VisualElement class="layout-row stars gap-8">
<ui:Label class="star" text="★"/>
<ui:Label class="star" text="★"/>
<ui:Label class="star" text="☆"/>
</ui:VisualElement>
</ui:VisualElement>
</ui:VisualElement>
</ui:VisualElement>
</ui:VisualElement>
