<ui:VisualElement class="cosmicelements-screen layout-column">
<ui:VisualElement class="cosmicelements-background"/>


<ui:VisualElement class="cosmicelements-frame layout-column responsive-pad gap-12" name="path-frame">
<ui:VisualElement class="header layout-column gap-8">
<ui:Label class="ce-title-lg" text="Level Select — Path"/>
<ui:Label class="ce-subtitle" text="Progress along the rift line"/>
</ui:VisualElement>


<ui:VisualElement class="layout-row gap-16">
